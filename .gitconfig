[user]
	email = -@users.noreply.github.com
	name = -
[core]
	autocrlf = false
	editor = vim
	pager = less -iXFR
[i18n]
	logOutputEncoding = utf-8
	commitEncoding = utf-8
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[difftool]
	prompt = false
[color]
	ui = true

